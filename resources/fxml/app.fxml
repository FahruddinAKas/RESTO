<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="500.0" prefWidth="800.0" stylesheets="@../css/style.css" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.unindra.restoclient.controllers.AppController">
    <HBox alignment="BOTTOM_LEFT" prefHeight="69.0" prefWidth="800.0" spacing="10.0">
        <ImageView fitHeight="50.0" fitWidth="50.0" onMousePressed="#pengaturanHandle" pickOnBounds="true" preserveRatio="true">
            <HBox.margin>
                <Insets right="10.0" />
            </HBox.margin>
            <Image url="@../icons/logo-ramen-bulet-merah-copy50x50.png" />
        </ImageView>
        <JFXButton fx:id="ramenButton" ellipsisString="R" focusTraversable="false" mnemonicParsing="false" onAction="#menuHandle" ripplerFill="WHITE" styleClass="button-header-pressed" text="Ramen">
            <font>
                <Font name="System Bold" size="12.0" />
            </font>
        </JFXButton>
        <JFXButton fx:id="minumanButton" ellipsisString="M" focusTraversable="false" layoutX="75.0" layoutY="45.0" mnemonicParsing="false" onAction="#menuHandle" ripplerFill="WHITE" styleClass="button-header" text="Minuman">
            <font>
                <Font name="System Bold" size="12.0" />
            </font>
        </JFXButton>
        <JFXButton fx:id="cemilanButton" ellipsisString="C" focusTraversable="false" mnemonicParsing="false" onAction="#menuHandle" ripplerFill="WHITE" styleClass="button-header" text="Cemilan">
            <font>
                <Font name="System Bold" size="12.0" />
            </font>
        </JFXButton>
        <JFXButton fx:id="lainnyaButton" ellipsisString="L" focusTraversable="false" mnemonicParsing="false" onAction="#menuHandle" ripplerFill="WHITE" styleClass="button-header" text="Lainnya">
            <font>
                <Font name="System Bold" size="12.0" />
            </font>
        </JFXButton>
        <Pane prefHeight="50.0" prefWidth="224.0" HBox.hgrow="ALWAYS" />
        <JFXButton fx:id="pesananButton" alignment="TOP_CENTER" ellipsisString="P" focusTraversable="false" mnemonicParsing="false" onAction="#pesananHandle" prefHeight="28.0" prefWidth="106.0" styleClass="pesanan-button" text="Pesanan" textAlignment="CENTER">
            <graphic>
                <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true" />
            </graphic>
            <font>
                <Font name="System Bold" size="12.0" />
            </font>
        </JFXButton>
        <padding>
            <Insets bottom="10.0" left="50.0" right="50.0" top="10.0" />
        </padding>
        <VBox.margin>
            <Insets />
        </VBox.margin>
    </HBox>
    <ScrollPane fx:id="mainPane" fitToHeight="true" fitToWidth="true" prefHeight="200.0" prefWidth="200.0" stylesheets="@../css/scroll.css" VBox.vgrow="ALWAYS" />
</VBox>
